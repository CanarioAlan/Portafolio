<section>
    <h2 class="text-2xl my-4">Mis tecnologia</h2>
    <div class="flex flex-wrap  gap-5 justify-center items-center sm:h-56" >
        <div class="group group-hover:scale-110 hover:scale-110 transition ">
            <i class='text-6xl bx bxl-html5 group-hover:text-yellow-200'></i>
            <p class="mt-2.5 group-hover:text-yellow-200">HTML 5</p>
        </div>
        <div class="group hover:scale-110 transition">
            <i class='text-6xl bx bxl-css3 group-hover:text-yellow-200'></i>
            <p class="mt-2.5 group-hover:text-yellow-200">CSS 3</p>
        </div>
        <div class="group hover:scale-110 transition">
            <i class='text-6xl bx bxl-tailwind-css group-hover:text-yellow-200'></i>
            <p class="mt-2.5 group-hover:text-yellow-200">Tailwind css</p>
        </div>
        <div class="group hover:scale-110 transition">
            <i class='text-6xl bx bxl-javascript group-hover:text-yellow-200'></i>
            <p class="mt-2.5 group-hover:text-yellow-200">JavaScript</p>
        </div>
        <div class="group hover:scale-110 transition">
            <i class='text-6xl bx bxl-nodejs group-hover:text-yellow-200'></i>
            <p class="mt-2.5 group-hover:text-yellow-200">Node.JS</p>
        </div>
        <div class="group hover:scale-110 transition">
            <i class='text-6xl bx bxl-php group-hover:text-yellow-200'></i>
            <p class="mt-2.5 group-hover:text-yellow-200">PHP</p>
        </div>
